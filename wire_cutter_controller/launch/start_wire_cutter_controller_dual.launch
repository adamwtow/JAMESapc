<launch>
    <!-- Start nozzle joint controller -->
    <rosparam file="$(find gripper_controller)/config/gripper_position_dual.yaml" command="load"/>
    <node name="gripper_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port motor_port
                gripper_controller1"
          output="screen"/>

</launch>

